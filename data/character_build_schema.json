{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "character_name": {
        "type": "string"
      },
      "guide_video_url": {
        "type": "string",
        "format": "uri"
      },
      "guide_image_url": {
        "type": "string",
        "format": "uri"
      },
      "character_icon_url": {
        "type": "string",
        "format": "uri"
      },
      "element": {
        "type": "string",
        "enum": ["pyro", "cryo", "hydro", "dendro", "anemo", "geo", "electro"]
      }
    },
    "required": [
      "character_name",
      "guide_image_url",
      "character_icon_url",
      "element"
    ],
    "additionalProperties": false
  }
}
